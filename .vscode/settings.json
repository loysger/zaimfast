{
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2,
  "editor.rulers": [80],
  "files.associations": {
    ".stylelintrc": "json",
    ".htmlhintrc": "json",
    ".eslintrc": "yaml",
    ".jsbeautifyrc": "json"
  },
  "[handlebars]": {
    "editor.rulers": [120]
  },
  "npm.packageManager": "yarn",
  "npm.enableScriptExplorer": true
}
